@model List<CoreIntro.Models.Kitap>
<table class="table table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>Isım</th>
            <th>Yazar</th>
        </tr>
    </thead>


    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.KitapIsmi</td>
                <td>@item.Yazar</td>
            </tr>
        }
    </tbody>

</table>